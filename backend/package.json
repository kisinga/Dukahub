{
  "name": "dukahub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "populate": "node ./dist/populate.js",
    "migration:run": "node -e \"require('@vendure/core').runMigrations(require('./dist/vendure-config').config).then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "entrypoint": "ts-node ./src/entrypoint.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:migrations": "jest --testNamePattern='Migration Idempotence'",
    "test:all": "npm run build && npm run test:coverage"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "3.4.3",
    "@vendure/asset-server-plugin": "3.4.3",
    "@vendure/core": "3.4.3",
    "@vendure/create": "3.4.3",
    "@vendure/email-plugin": "3.4.3",
    "@vendure/graphiql-plugin": "3.4.3",
    "concurrently": "9.2.1",
    "dotenv": "17.2.3",
    "pg": "8.16.3",
    "type-graphql": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/pg": "^8.15.5",
    "@vendure/cli": "3.4.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3"
  }
}
